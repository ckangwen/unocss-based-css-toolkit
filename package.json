{
  "name": "unocss-based-css-toolkit",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "keywords": [
    "unocss",
    "unocss-preset",
    "css-framework"
  ],
  "license": "MIT",
  "scripts": {
    "build": "pnpm run build --filter=my-unocss-preset-ui",
    "lint": "eslint --fix",
    "prepare": "husky install",
    "start:docs": "pnpm run dev --filter=docs",
    "start:playground": "pnpm run dev --filter=playground",
    "test": "vitest"
  },
  "lint-staged": {
    "*.ts": "pnpm run lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/rimraf": "^3.0.2",
    "@unocss/core": "^0.44.3",
    "@unocss/vite": "^0.44.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vueuse/core": "^8.7.5",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.3.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "typescript": "^4.5.2",
    "vite": "2.9.14",
    "vitest": "latest",
    "vue": "^3.2.37"
  }
}