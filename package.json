{
  "name": "unocss-css-framework-preset-template",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "unocss",
    "unocss-preset",
    "css-framework"
  ],
  "license": "MIT",
  "scripts": {
    "build": "pnpm run build --filter=preset",
    "start:playground": "pnpm run dev --filter=playground",
    "start:docs": "pnpm run dev --filter=docs",
    "lint": "eslint --fix",
    "prepare": "husky install",
    "test": "vitest"
  },
  "lint-staged": {
    "*.ts": "pnpm run lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@vueuse/core": "^8.7.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.3.0",
    "prettier": "^2.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "typescript": "^4.5.2",
    "vite": "2.9.12",
    "vue": "^3.2.37",
    "vitest": "latest"
  }
}